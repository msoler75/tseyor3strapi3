{
  "kind": "collectionType",
  "collectionName": "eventos",
  "info": {
    "name": "Eventos",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titulo": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "titulo"
    },
    "texto": {
      "type": "richtext"
    },
    "descripcion": {
      "type": "text",
      "maxLength": 1024,
      "required": true,
      "minLength": 8
    },
    "fechaComienzo": {
      "type": "datetime"
    },
    "fechaFinal": {
      "type": "datetime"
    },
    "imagen": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "imagenes": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "tipoEvento": {
      "type": "enumeration",
      "enum": [
        "curso",
        "encuentro",
        "presentacion"
      ]
    },
    "sala": {
      "via": "eventos",
      "model": "salas"
    },
    "asistentes": {
      "via": "asistire",
      "plugin": "users-permissions",
      "collection": "user"
    },
    "organiza": {
      "model": "centros",
      "via": "eventos"
    },
    "zonahoraria": {
      "type": "enumeration",
      "enum": [
        "Argentina",
        "Espana",
        "Mexico",
        "Chile",
        "Colombia",
        "Venezuela"
      ],
      "default": "Espana"
    },
    "autor": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "eventos"
    }
  }
}
