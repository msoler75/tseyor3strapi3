{
  "kind": "collectionType",
  "collectionName": "users-permissions_user",
  "info": {
    "name": "user",
    "description": ""
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "model": "role",
      "via": "users",
      "plugin": "users-permissions",
      "configurable": false
    },
    "nombreSimbolico": {
      "type": "string"
    },
    "grupos": {
      "collection": "grupos",
      "via": "users",
      "dominant": true
    },
    "imagen": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "frase": {
      "type": "string"
    },
    "asiste": {
      "collection": "eventos",
      "via": "asistentes",
      "dominant": true
    },
    "likes": {
      "via": "user",
      "collection": "likes"
    },
    "likeit": {
      "via": "likes",
      "collection": "comentarios"
    },
    "notificacionesEquipos": {
      "via": "suscriptores",
      "collection": "equipos"
    },
    "coordina": {
      "via": "coordinadores",
      "collection": "equipos"
    },
    "equipos": {
      "via": "miembros",
      "collection": "equipos",
      "dominant": true
    },
    "comentarios": {
      "via": "autor",
      "collection": "comentarios"
    },
    "eventos": {
      "via": "autor",
      "collection": "eventos"
    },
    "historial": {
      "via": "autor",
      "collection": "historial"
    },
    "recopilaciones": {
      "via": "autor",
      "collection": "recopilaciones"
    },
    "archivosSubidos": {
      "via": "subidoPor",
      "collection": "archivos"
    }
  }
}
